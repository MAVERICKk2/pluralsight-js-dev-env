{
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    // 0 Off, 1 Warning, 2 Error, Override eslint predefined rules
    "rules": {
        "no-console": 1
    }
}
